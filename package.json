{
  "name": "job_tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview --host 0.0.0.0 --port $PORT",
    "start": "npm run preview",
    "railway:build": "npm ci && npm run build",
    "railway:start": "npm run preview",
    "backup:env": "cp .env .env.backup.$(date +%Y%m%d-%H%M%S)",
    "backup:repo": "git archive --format=zip --output=backup/repo-backup-$(date +%Y%m%d-%H%M%S).zip HEAD",
    "test:generate-data": "node scripts/generate-test-data.js",
    "test:benchmark": "node scripts/performance-benchmark.js",
    "test:stress": "node scripts/stress-tests.js",
    "test:monitor": "node scripts/monitoring-dashboard.js",
    "test:full-suite": "npm run test:generate-data 10000 && npm run test:benchmark && npm run test:stress",
    "test:railway:validate": "node scripts/railway-deployment-validator.js",
    "test:railway:performance": "node scripts/railway-performance-benchmark.js",
    "test:railway:rollback": "node scripts/rollback-testing-suite.js",
    "test:railway:monitoring": "node scripts/monitoring-health-checks.js",
    "test:railway:uat": "node scripts/user-acceptance-testing.js",
    "test:railway:all": "node scripts/railway-test-suite-orchestrator.js",
    "deploy:prepare": "npm run backup:env && npm run backup:repo && git checkout -b railway-deployment",
    "deploy:tag": "git tag -a v1.0.0-railway-$(date +%Y%m%d) -m 'Railway deployment version'",
    "deploy:backup-db": "node scripts/database-backup.js",
    "deploy:validate-db": "node scripts/database-validation.js",
    "deploy:rollback": "node scripts/rollback-strategy.js",
    "migration:full": "npm run deploy:backup-db && npm run deploy:validate-db",
    "health:check": "curl -f http://localhost:$PORT/health || exit 1"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@supabase/supabase-js": "^2.50.5",
    "framer-motion": "^12.23.12",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "express": "^4.19.2",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "puppeteer": "^23.11.1",
    "tailwindcss": "^4.1.11",
    "terser": "^5.44.0",
    "vite": "^7.0.4",
    "ws": "^8.18.0"
  }
}
